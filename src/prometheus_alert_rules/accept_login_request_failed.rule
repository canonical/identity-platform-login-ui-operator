alert: AcceptLoginRequestFailed
expr: sum(increase(http_response_time_seconds_count{route="GET/api/kratos/self-service/login/browser", status="500"}[2m])) > 0
labels:
  severity: warning
annotations:
  message: "Error in application {{ $labels.juju_application }} of Juju charm {{ $labels.juju_charm }} in model {{ $labels.juju_model }}. Login Request Failed."